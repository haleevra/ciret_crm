		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	_1ГруппаКлиентов.Значение КАК ГруппаКлиентов,
	Партнеры.Ссылка КАК Клиент,
	Партнеры.ОсновнойМенеджер КАК KAM,
	Партнеры.БизнесРегион КАК Регион,
	_5Status.Значение КАК Статус
ПОМЕСТИТЬ ВТ_Классификация
ИЗ
	Справочник.Партнеры КАК Партнеры
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Партнеры.ДополнительныеРеквизиты КАК _1ГруппаКлиентов
		ПО Партнеры.Ссылка = _1ГруппаКлиентов.Ссылка
			И (_1ГруппаКлиентов.Свойство.Заголовок = "Группа клиентов")
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ДополнительныеСведения КАК _5Status
		ПО Партнеры.Ссылка = _5Status.Объект
			И (_5Status.Свойство.Заголовок = "Status")
ГДЕ
	Партнеры.Клиент
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ПланПродажПоКатегориямТоварныеКатегории.ТоварнаяКатегория КАК ГруппаТоваров,
	ЕСТЬNULL(ВТ_Классификация.ГруппаКлиентов, "Пусто") КАК ГруппаКлиентов,
	ВТ_Классификация.Клиент КАК Клиент,
	ЕСТЬNULL(ВТ_Классификация.KAM, "Пусто") КАК КАМ,
	ЕСТЬNULL(ВТ_Классификация.Регион, "Пусто") КАК Регион,
	ЕСТЬNULL(ВТ_Классификация.Статус, "Пусто") КАК Статус,
	СУММА(ПланПродажПоКатегориямТоварныеКатегории.Количество) * РАЗНОСТЬДАТ(&amp;ДатаНачала, &amp;ДатаТекущая, ДЕНЬ) / РАЗНОСТЬДАТ(&amp;ДатаНачала, &amp;ДатаОкончания, ДЕНЬ) КАК План
ПОМЕСТИТЬ ВТ_План
ИЗ
	Документ.ПланПродажПоКатегориям.ТоварныеКатегории КАК ПланПродажПоКатегориямТоварныеКатегории
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ДополнительныеСведения КАК ДополнительныеСведения
		ПО ПланПродажПоКатегориямТоварныеКатегории.Ссылка = ДополнительныеСведения.Объект
			И (ДополнительныеСведения.Свойство.Заголовок = "Клиент")
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Классификация КАК ВТ_Классификация
		ПО (ДополнительныеСведения.Значение = ВТ_Классификация.Клиент)
ГДЕ
	ПланПродажПоКатегориямТоварныеКатегории.Ссылка.НачалоПериода &gt;= &amp;ДатаНачала
	И ПланПродажПоКатегориямТоварныеКатегории.Ссылка.ОкончаниеПериода &lt;= &amp;ДатаОкончания
	И ПланПродажПоКатегориямТоварныеКатегории.Ссылка.Проведен

СГРУППИРОВАТЬ ПО
	ЕСТЬNULL(ВТ_Классификация.ГруппаКлиентов, "Пусто"),
	ПланПродажПоКатегориямТоварныеКатегории.ТоварнаяКатегория,
	ЕСТЬNULL(ВТ_Классификация.KAM, "Пусто"),
	ЕСТЬNULL(ВТ_Классификация.Регион, "Пусто"),
	ЕСТЬNULL(ВТ_Классификация.Статус, "Пусто"),
	ВТ_Классификация.Клиент
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВыручкаИСебестоимостьПродажОбороты.АналитикаУчетаНоменклатуры.Номенклатура.ТоварнаяКатегория КАК ГруппаТоваров,
	ЕСТЬNULL(ВТ_Классификация.ГруппаКлиентов, "Пусто") КАК ГруппаКлиентов,
	ВТ_Классификация.Клиент КАК Клиент,
	ЕСТЬNULL(ВТ_Классификация.KAM, "Пусто") КАК КАМ,
	ЕСТЬNULL(ВТ_Классификация.Регион, "Пусто") КАК Регион,
	ЕСТЬNULL(ВТ_Классификация.Статус, "Пусто") КАК Статус,
	СУММА(ВыручкаИСебестоимостьПродажОбороты.СуммаВыручкиОборот) КАК Продажи
ПОМЕСТИТЬ ВТ_Факт
ИЗ
	РегистрНакопления.ВыручкаИСебестоимостьПродаж.Обороты(&amp;ДатаНачала, &amp;ДатаОкончания, , ) КАК ВыручкаИСебестоимостьПродажОбороты
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Классификация КАК ВТ_Классификация
		ПО ВыручкаИСебестоимостьПродажОбороты.АналитикаУчетаПоПартнерам.Партнер = ВТ_Классификация.Клиент

СГРУППИРОВАТЬ ПО
	ВыручкаИСебестоимостьПродажОбороты.АналитикаУчетаНоменклатуры.Номенклатура.ТоварнаяКатегория,
	ЕСТЬNULL(ВТ_Классификация.ГруппаКлиентов, "Пусто"),
	ЕСТЬNULL(ВТ_Классификация.KAM, "Пусто"),
	ЕСТЬNULL(ВТ_Классификация.Регион, "Пусто"),
	ЕСТЬNULL(ВТ_Классификация.Статус, "Пусто"),
	ВТ_Классификация.Клиент
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ЕСТЬNULL(ВТ_План.ГруппаТоваров, ВТ_Факт.ГруппаТоваров) КАК ГруппаТоваров,
	ЕСТЬNULL(ВТ_План.ГруппаКлиентов, ВТ_Факт.ГруппаКлиентов) КАК ГруппаКлиентов,
	ЕСТЬNULL(ВТ_План.Клиент, ВТ_Факт.Клиент) КАК Клиент,
	ЕСТЬNULL(ВТ_План.КАМ, ВТ_Факт.КАМ) КАК КАМ,
	ЕСТЬNULL(ВТ_План.Регион, ВТ_Факт.Регион) КАК Регион,
	ЕСТЬNULL(ВТ_План.Статус, ВТ_Факт.Статус) КАК Статус,
	ВТ_План.План КАК План,
	ВТ_Факт.Продажи КАК Факт
ИЗ
	ВТ_План КАК ВТ_План
		ПОЛНОЕ СОЕДИНЕНИЕ ВТ_Факт КАК ВТ_Факт
		ПО ВТ_План.ГруппаТоваров = ВТ_Факт.ГруппаТоваров
			И ВТ_План.ГруппаКлиентов = ВТ_Факт.ГруппаКлиентов
			И ВТ_План.КАМ = ВТ_Факт.КАМ
			И ВТ_План.Регион = ВТ_Факт.Регион
			И ВТ_План.Статус = ВТ_Факт.Статус
			И ВТ_План.Клиент = ВТ_Факт.Клиент</query>
	</dataSet>
	<totalField>
		<dataPath>План</dataPath>
		<expression>Сумма(План)</expression>
	</totalField>
	<totalField>
		<dataPath>Факт</dataPath>
		<expression>Сумма(Факт)</expression>
	</totalField>
